{
  "manifest_version": 2,
  "name": "Songsterr Unblocker",
  "version": "1.1",
  "description": "Removes interruptions from songsterr.com tabs.",
  "icons": {
    "48": "icons/zero_three_five_48.png",
    "96": "icons/zero_three_five_96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.songsterr.com/*"
      ],
      "js": [
        "remove_interruption.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{fc5c8f5b-cadb-41e5-8723-fbeb54d30052}"
    }
  }
}